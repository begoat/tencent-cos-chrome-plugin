{
  "name": "tencent-cos-chrome-plugin",
  "version": "0.1.0",
  "description": "Chrome Plugin For Tencent COS",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --colors --mode development --open",
    "build": "rimraf build && cross-env NODE_ENV=production webpack --mode production",
    "lint": "npm run lint.js && npm run lint.style",
    "lint.js": "eslint -c .eslintrc.js --no-eslintrc --ext .ts,.tsx src",
    "lint.style": "stylelint \"src/**/*.less\"",
    "lint:fix": "eslint -c .eslintrc.js --no-eslintrc --ext .ts,.tsx --fix src",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "deploy": "gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/begoat/tencent-cos-chrome-plugin.git"
  },
  "keywords": [
    "cos",
    "tencent",
    "plugin",
    "chrome"
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-proposal-optional-chaining": "7.6.0",
    "@babel/plugin-transform-proto-to-assign": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.7.1",
    "@babel/preset-react": "7.7.0",
    "@babel/preset-typescript": "7.7.2",
    "@types/chrome": "^0.0.114",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "25.2.2",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "2.7.0",
    "@typescript-eslint/parser": "2.7.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-dev": "^2.0.1",
    "babel-preset-rsuite": "^4.0.1",
    "copy-webpack-plugin": "^6.0.1",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "cz-conventional-changelog": "3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-import-resolver-typescript": "1.1.1",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.11.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "gh-pages": "^3.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "4.2.2",
    "jest": "25.5.4",
    "less": "^3.11.1",
    "less-loader": "^6.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "ncp": "^2.0.0",
    "prettier": "2.0.5",
    "react-hot-loader": "^4.12.21",
    "react-test-renderer": "^16.13.1",
    "regenerator-runtime": "^0.13.5",
    "rimraf": "^3.0.2",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "25.5.1",
    "typescript": "^3.8.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "3.10.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 10"
  ],
  "dependencies": {
    "cos-js-sdk-v5": "^0.5.26",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "rsuite": "^4.5.0"
  }
}
